{
	"name": "auto-using",
	"displayName": "Auto-Using for C#",
	"icon": "icon.png",
	"version": "0.7.3",
	"publisher": "Fudge",
	"description": "Provides intellisense for and imports references from all available sources.",
	"repository": {
		"type": "git",
		"url": "https://github.com/natanfudge/Auto-Using"
	},
	"bugs": {
		"url": "https://github.com/natanfudge/Auto-Using/issues"
	},
	"files": [
		"out/**",
		"!out/test/**"
	],
	"license": "SEE LICENSE IN LICENSE",
	"keywords": [
		"C#",
		"csharp",
		"import",
		"require",
		"using",
		"auto-using",
		"auto-import"
	],
	"engines": {
		"vscode": "^1.18.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.wipeCommon",
				"title": "Wipe Memory Of Commonly Used References"
			}
		],
		"keybindings": [
			{
				"command": "extension.wipeCommon",
				"key": "Ctrl+Shift+W"
			}
		]
	},
	"activationEvents": [
		"onLanguage:csharp"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^6.0.40",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2",
		"vscode": "^1.1.17"
	},
	"extensionDependencies": [
		"ms-vscode.csharp"
	],
	"dependencies": {
		"@types/mocha": "^5.2.5",
		"colors": "^1.3.3",
		"i": "^0.3.6",
		"perf_hooks": "0.0.1",
		"ts-node": "^7.0.1"
	}
}
